{
    "name": "codiiv/laravel-taxonomies",
    "description": "Taxonomies for Laravel (Similar to Wordpress)",
    "homepage": "https://github.com/codiiv/laravel-taxonomies",
    "keywords": [
        "taxonomy",
        "taxonomies",
        "laravel taxonomies",
        "laravel tags",
        "laravel locations"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Codiiv",
            "email": "codiivdev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Codiiv\\Taxonomies\\": "/src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
      },
      "extra": {
          "laravel": {
              "providers": [
                  "Codiiv\\Taxonomies\\TaxonomiesServiceProvider"
              ]
          }
      },
      "minimum-stability": "stable",
      "require": {
        "laravel/framework": "^6.0 || ^7.0 || ^8.0 || ^9.0"
        "codiiv/extrameta": ">=1.1"
      },
      "require-dev": {
          "laravel/laravel": "^6.0 || ^7.0 || ^8.0 || ^9.0"
          "codiiv/extrameta": ">=1.1"
          
    }
}
